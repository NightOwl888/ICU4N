<Project Sdk="Microsoft.NET.Sdk">
  
  <PropertyGroup>
    <RootNamespace>ICU4N</RootNamespace>
  </PropertyGroup>

  <PropertyGroup>
    <NoWarn>$(NoWarn);1701;1702</NoWarn>
  </PropertyGroup>

  <PropertyGroup Label="Assembly Publishing">
    <IsPublishable Condition=" '$(UseSatelliteAssemblies.ToLowerInvariant())' == 'true' ">true</IsPublishable>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="../../src/ICU4N/ICU4N.csproj" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="$(MSBuildThisFileDirectory)/Dev/Data/**/*" LinkBase="Dev/Data" />
    <EmbeddedResource Include="$(MSBuildThisFileDirectory)/Dev/Test/Rbbi/**/*" LinkBase="Dev/Test/Rbbi" />
    <EmbeddedResource Include="$(MSBuildThisFileDirectory)/Dev/Test/Util/**/*.tri2" LinkBase="Dev/Test/Util" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Support/Text/UnicodeSetPartialTestExtension.tt">
      <Generator>TextTemplatingFileGenerator</Generator>
      <LastGenOutput>UnicodeSetPartialTestExtension.cs</LastGenOutput>
    </None>
  </ItemGroup>
  
  <ItemGroup>
    <Service Include="{508349b6-6b84-4df5-91f0-309beebad82d}" />
  </ItemGroup>
  
  <ItemGroup>
    <Compile Update="Support/Text/UnicodeSetPartialTestExtension.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>UnicodeSetPartialTestExtension.tt</DependentUpon>
    </Compile>
  </ItemGroup>

</Project>